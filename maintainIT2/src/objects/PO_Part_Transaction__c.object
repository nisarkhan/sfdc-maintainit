        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/apex.js&quot;)}

// Get the List of Schedule Ids selected by the User.
var records= {!GETRECORDIDS($ObjectType.PM_Schedule__c)};

var len=records.length;

//  Call the method if User has selected any record.
if (len!= 0){
           alert(&quot;Please wait while request is processing... &quot;);
            var res=sforce.apex.execute(&quot;sm1e.CloneWorkOrder_v2&quot;,&quot;CloneworkorderSchedule&quot;,{scheduleId:records});
      }else{
              window.alert(&quot;At least one PM Schedule must be selected&quot;);
     }

// Reload the Window..

if(res==&quot;true&quot;){
        window.alert(&quot;PM Work Order Generation Submitted&quot;);
        window.location.reload();
          
}</url>
