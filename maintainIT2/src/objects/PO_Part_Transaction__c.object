    <validationRules>
        <fullName>Calendar_Scheduling_Interval_Positive</fullName>
        <active>true</active>
        <description>Ensures that the Calendar Scheduling Interval is a Positive Value</description>
        <errorConditionFormula>Scheduling_Interval__c  &lt;= 0</errorConditionFormula>
        <errorDisplayField>Scheduling_Interval__c</errorDisplayField>
        <errorMessage>The Calendar Scheduling Interval must be greater than or equal to one (1)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_PMStatus_is_Draft</fullName>
        <active>true</active>
        <description>Ensures that the initial value of the PM Schedule Status picklist is &quot;Draft&quot;</description>
        <errorConditionFormula>IF( ISNEW(),  IF(TEXT( PM_Status__c)  &lt;&gt;  &quot;Draft&quot;, True, False) , False)</errorConditionFormula>
        <errorDisplayField>PM_Status__c</errorDisplayField>
        <errorMessage>The PM Schedule must be created with an initial status of Draft.  You may create a Workflow process or manually change the status after the PM Schedule is created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limits_MBS_to_Floating_CBS</fullName>
        <active>true</active>
        <description>Limits the overlay of Meter Based Scheduling to Floating Interval Calendar Based Scheduling</description>
        <errorConditionFormula>IF( SBC__c = True &amp;&amp;  FI__c = False &amp;&amp;  SBM__c = True, true, false)</errorConditionFormula>
        <errorMessage>Meter Based Scheduling cannot be used in conjunction with Fixed Interval scheduling.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meter_Scheduling_Interval_Positive</fullName>
        <active>true</active>
        <description>Ensures that the Meter Scheduling Interval is a positive value</description>
        <errorConditionFormula>IF( SBM__c  = True,  MSI__c  &lt;= 0, false)</errorConditionFormula>
        <errorDisplayField>MSI__c</errorDisplayField>
        <errorMessage>The Meter Scheduling Interval must be greater than zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Meter_is_Selected</fullName>
        <active>true</active>
        <description>Ensures that a Meter ID has been input if &quot;Schedule by Meter&quot; is selected</description>
        <errorConditionFormula>IF(( SBM__c = True) ,  ISBLANK( Meter_ID__c ) , false)</errorConditionFormula>
        <errorDisplayField>Meter_ID__c</errorDisplayField>
        <errorMessage>A Meter ID must be specified if Schedule by Meter is selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>One_Scheduling_Method</fullName>
        <active>true</active>
        <description>Ensures only one scheduling method, Calendar or Meter, is selected</description>
        <errorConditionFormula>IF( SBC__c = True &amp;&amp;  SBM__c = True, true, false)</errorConditionFormula>
        <errorMessage>Only one scheduling method, Calendar or Meter, can be selected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_Auto_Schedule_for_Calendar_Floating</fullName>
        <active>false</active>
        <description>Ensures that the Auto Schedule attribute can be set to True for ONLY Calendar based Floating Interval PM Schedules. (Obsolete - Do Not Activate)</description>
        <errorConditionFormula>IF( Auto_Schedule__c = True  &amp;&amp; (  SBC__c  = False ||  FI__c = False), true, false)</errorConditionFormula>
        <errorDisplayField>Auto_Schedule__c</errorDisplayField>
        <errorMessage>Auto Schedule may be activated for only Schedule by Calendar, Floating Interval PM Schedules</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Scheduling_Method_Selected</fullName>
        <active>true</active>
        <description>Ensures that a Meter Scheduling Method is selected if the PM Schedule Status is Active</description>
        <errorConditionFormula>IF(ISPICKVAL( PM_Status__c , &quot;Active&quot;) , ( SBC__c = False  &amp;&amp;  SBM__c  = False), false)</errorConditionFormula>
        <errorDisplayField>PM_Status__c</errorDisplayField>
        <errorMessage>Schedule by Calendar or Schedule by Meter MUST be selected for PM Schedule Status to be Active</errorMessage>
    </validationRules>
   
