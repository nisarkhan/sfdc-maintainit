    <validationRules>
        <fullName>Num_People_NonNegative</fullName>
        <active>true</active>
        <description>Esures that the Number of People identified on a Work Order Task non-negative value</description>
        <errorConditionFormula>People_Required__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>People_Required__c</errorDisplayField>
        <errorMessage>The # of People on a work order task must be greater than or equal to zero (0)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Plan_Hours_NonNegative</fullName>
        <active>true</active>
        <description>Ensures that the Plan Hours on a Work Order Task is a non-negative value</description>
        <errorConditionFormula>Planned_Hours__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>Planned_Hours__c</errorDisplayField>
        <errorMessage>The Plan Hrs must be greater than or equal to zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WO_TaskPercentage_Completion</fullName>
        <active>true</active>
        <description>Constrains the % completion to be a non-negative value between 0 and 100</description>
        <errorConditionFormula>Percent_Complete__c &gt; 1 ||  Percent_Complete__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Percent_Complete__c</errorDisplayField>
        <errorMessage>The Percentage Completion must be between 0 and 100</errorMessage>
    </validationRules>
