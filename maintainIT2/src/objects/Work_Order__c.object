        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/15.0/connection.js&quot;)} 

// Get the List of Work Order Ids selected by the User. 

var records= {!GETRECORDIDS($ObjectType.Work_Order__c)}; 


var len=records.length; 


// Call the method if User has selected any record. 
if (len!= 0){ 

window.open(&quot;/apex/WorkOrdersMapView?records=&quot; +records); 
}else{ 
window.alert(&quot;Atleast one record should be selected before viewing the Asset Map......&quot;); 
}</url>
