    <validationRules>
        <fullName>Initial_Est_Hours_NonNegative</fullName>
        <active>true</active>
        <description>Ensures that the Initial Estimate Hours is a non-negative value</description>
        <errorConditionFormula>Initial_Est_Hours__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>Initial_Est_Hours__c</errorDisplayField>
        <errorMessage>The Initial Est Hours must be greater than or equal to zero (0)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WO_VST_Comp_to_Comp</fullName>
        <active>true</active>
        <description>Ensures that a work order Status of Closed can only be set from a previous status of Completed.</description>
        <errorConditionFormula>IF( ISPICKVAL(Status__c , &quot;Closed&quot;) &amp;&amp;  ISCHANGED(Status__c) &amp;&amp;  TEXT(PRIORVALUE( Status__c )) &lt;&gt; &quot;Completed&quot; , true, false)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A work order status of Closed can only be set from a prior status of Completed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Work_Order_Completion_Date_Not_In_Future</fullName>
        <active>true</active>
        <description>Prohibits the work order completion date from being updated to a future date</description>
        <errorConditionFormula>IF( Completion_Date__c &gt;  TODAY() , True, False)</errorConditionFormula>
        <errorDisplayField>Completion_Date__c</errorDisplayField>
        <errorMessage>The Work Order Completion Date cannot be set to a future date</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Asset_BOM</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Asset BOMS</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!requireScript(&apos;/soap/ajax/27.0/connection.js&apos;)} 

var windowRef; 
var intervalId = 0; 

var url = window.location.protocol + &apos;//&apos; + window.location.host+&apos;/apex/LookupItemMasterBOM?id={!Work_Order__c.Id}&apos;;
 
setTimeout(checkToOpenWindow, 2*1000); 

function checkToOpenWindow(){ 

windowRef = window.open(url,&apos;_blank&apos;,&apos;location=1,status=1,scrollbars=1, resizable=1, height=650px, width=1000px, titlebar=yes&apos;,true);
 
checkWindowStatus(); 

} 


function checkWindowStatus() 
{ 
intervalId = setInterval(checkAndCloseWin, 5*1000); 
} 

function checkAndCloseWin() 
{ 
if(windowRef.closed) 
{ 
clearInterval(intervalId); 
window.parent.location.href = window.parent.location.href; 
} 
}</url>
    </webLinks>
    <webLinks>
